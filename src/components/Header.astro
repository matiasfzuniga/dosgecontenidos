<body>
  <div class="header">
    <img class="logo-gg" src="gg.png" alt="dosge" />
    <div class="menu-toggle" id="mobile-menu">
      <span class="bar"></span>
      <span class="bar"></span>
      <span class="bar"></span>
    </div>
  </div>

  <nav class="nav" id="nav">
    <ul>
      <li><a href="#">Inicio</a></li>
      <li><a href="#">Acerca de</a></li>
      <li><a href="#">productos</a></li>
      <li><a href="#">bodas</a></li>
      <li><a href="#">corporativos</a></li>
      <li><a href="#">quinces</a></li>
      <li><a href="#">Contacto</a></li>
    </ul>
  </nav>

  <script>
    document.getElementById("mobile-menu")
      .addEventListener("click", function () {
        var nav = document.getElementById("nav");
        nav.style.display = nav.style.display === "block" ? "none" : "block";
      });
  </script>
</body>
<style>
  body {
    margin: 0;
    font-family: "Arial", sans-serif;
  }
  .header {
    display: flex;
    justify-content: space-between;
    padding-left: 45px;
    padding-right: 45px;
    padding-top: 20px;
  }

  .logo-gg {
    width: 40px;
    height: 40px;
    padding: 15px;
  }

  .menu-toggle {
    display: block;
    cursor: pointer;
    padding: 15px;
  }

  .bar {
    display: block;
    width: 25px;
    height: 3px;
    background-color: #333;
    margin: 5px 0;
    transition: 0.4s;
  }

  .nav {
    display: none;
  }

  .nav ul {
    list-style: none;
    margin: 0;
    padding: 0;
    background-color: #333;
    text-align: center;
  }

  .nav li {
    padding: 15px;
  }

  .nav a {
    text-decoration: none;
    color: white;
    font-size: 18px;
  }

  @media screen and (max-width: 768px) {
    .nav {
      display: block;
    }

    .nav ul {
      display: flex;
      flex-direction: column;
    }

    .nav li {
      border-bottom: 1px solid #555;
    }
    .header {
      padding: 20px 20px 20px;
    }
    .logo-gg {
      padding: 0px;
    }
    .menu-toggle {
      padding: 0px;
    }
  }
</style>
